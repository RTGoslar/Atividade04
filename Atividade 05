//Faça um programa que imprima todos os números pares compreeendidos entre 85 e 907. O programa deve também calcular a soma destes valores.

            int inicio = 85, final = 907, soma = 0, count = 0;

            for (int i = inicio; i <= final; i++)
            {
                if (i % 2 == 0)
                {
                    soma = soma + i;
                    count++;
                }
            }

            Console.WriteLine($"A soma dos números pares entre 85 e 907 é {soma}, e a quantidade de números positivos é {count},");

            Console.ReadKey();